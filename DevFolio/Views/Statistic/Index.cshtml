
@{
	ViewBag.Title = "Index";
	Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="alert alert-danger">Kategori Sayısı: @ViewBag.categoryCount</div>
<div class="alert alert-warning">Proje Sayısı: @ViewBag.projeCount</div>
<div class="alert alert-success">Yetenek Sayısı: @ViewBag.skillCount</div>
<div class="alert alert-secondary">Hizmet Sayısı: @ViewBag.servicesCount</div>
<div class="alert alert-dark">Ortalama Yetenek Puanı: @ViewBag.skillAvgValue</div>
<div class="alert alert-primary">Son Eklenen Yetenek: @ViewBag.lastSkillTitleName</div>
<div class="alert alert-info">Asp.Net Core Proje Sayısı: @ViewBag.coreCategoryProjectCount</div>
<div class="alert alert-light">Yönetici Sayısı: @ViewBag.adminCount </div>
<div class="alert alert-success">Mesaj Sayısı: @ViewBag.messageCount </div>
<div class="alert alert-secondary">Okunmamış Mesaj Sayısı: @ViewBag.unreadMessageCount </div>



