@model List<DevFolio.Models.TblAddress>

<div class="title-box-2 pt-4 pt-md-0">
	<h5 class="title-left">
		Bize Ulaşın
	</h5>
</div>
@foreach (var item in Model)
{
<div class="more-info">
	<p class="lead">
		@item.Description
	</p>
	<ul class="list-ico">
		<li><span class="ion-ios-location"></span> @item.Location</li>
		<li><span class="ion-ios-telephone"></span> @item.PhoneNumber</li>
		<li><span class="ion-email"></span> @item.Email</li>
	</ul>
</div>
}